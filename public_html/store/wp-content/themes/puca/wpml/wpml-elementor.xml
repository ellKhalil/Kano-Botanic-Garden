<wpml-config>
    <elementor-widgets>
        <widget name="tbay-brands">
            <fields>
                <field type="Brands: Title" editor_type="LINE">heading_title</field>
                <field type="Brands: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-button">
            <fields>
                <field type="Text Button:" editor_type="LINE">text_button</field>
            </fields>
        </widget>

        <widget name="tbay-features">
            <fields-in-item items_of="features">
                <field type="Features Item: Title" editor_type="LINE">feature_title</field>
                <field type="Features Item: Description" editor_type="AREA">feature_desc</field>
            </fields-in-item>
        </widget>

        <widget name="tbay-heading">
            <fields>
                <field type="Heading: Title" editor_type="LINE">heading_title</field>
                <field type="Heading: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Heading: Description" editor_type="AREA">heading_description</field>
            </fields>
        </widget>

        <widget name="tbay-nav-menu">
            <fields>
                <field type="Nav Menu: Title" editor_type="LINE">heading_title</field>
            </fields>
        </widget>

        <widget name="tbay-newsletter">
            <fields>
                <field type="Newsletter: Title" editor_type="LINE">heading_title</field>
                <field type="Newsletter: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Newsletter: Description" editor_type="AREA">heading_description</field>
            </fields>
        </widget>

        <widget name="tbay-our-team">
            <fields>
                <field type="Our Team: Title" editor_type="LINE">heading_title</field>
                <field type="Our Team: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="our_team">
                <field type="Our Team Item: Name" editor_type="LINE">our_team_name</field>
                <field type="Our Team Item: Job" editor_type="AREA">our_team_job</field>
            </fields-in-item>
        </widget>

        <widget name="tbay-posts-grid">
            <fields>
                <field type="Post: Title" editor_type="LINE">heading_title</field>
                <field type="Post: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-social-icons">
            <fields>
                <field type="Social: Title" editor_type="LINE">heading_title</field>
                <field type="Social: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-testimonials">
            <fields>
                <field type="Testimonials: Title" editor_type="LINE">heading_title</field>
                <field type="Testimonials: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-video">
            <fields>
                <field type="Video: Title" editor_type="LINE">heading_title</field>
                <field type="Video: Button Text" editor_type="LINE">video_btn_text</field>
            </fields>
        </widget>

        <widget name="tbay-custom-image-list-categories">
            <fields>
                <field type="Custom Image List Categories: Title" editor_type="LINE">heading_title</field>
                <field type="Custom Image List Categories: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-list-categories-product">
            <fields>
                <field type="List Categories: Title" editor_type="LINE">heading_title</field>
                <field type="List Categories: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-product-categories-tabs">
            <fields>
                <field type="Product Categories Tabs: Title" editor_type="LINE">heading_title</field>
                <field type="Product Categories Tabs: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="View All: Text" editor_type="LINE">view_all_text</field>
            </fields>
        </widget>

        <widget name="tbay-product-category">
            <fields>
                <field type="Product Category: Title" editor_type="LINE">heading_title</field>
                <field type="Product Category: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-product-count-down">
            <fields>
                <field type="Product Count Down: Title" editor_type="LINE">heading_title</field>
                <field type="Product Count Down: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-product-tabs">
            <fields>
                <field type="Product Tabs: Title" editor_type="LINE">heading_title</field>
                <field type="Product Tabs: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <widget name="tbay-products">
            <fields>
                <field type="Product: Title" editor_type="LINE">heading_title</field>
                <field type="Product: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text" editor_type="LINE">text_button</field>
            </fields>
        </widget>

        <widget name="tbay-woocommerce-tags">
            <fields>
                <field type="Woocommerce Tags: Title" editor_type="LINE">heading_title</field>
                <field type="Woocommerce Tags: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
        </widget>

        <!-- fashion 2 -->
        <widget name="tbay-fashion2-banner">
            <fields>
                <field type="Banner: Title" editor_type="LINE">banner_title</field>
                <field type="Banner: Sub Title" editor_type="LINE">banner_sub_title</field>
                <field type="Button 01: Text" editor_type="LINE">btn_text_1</field>
                <field type="Button 02: Text" editor_type="LINE">btn_text_2</field>
            </fields>
        </widget>

        <widget name="tbay-fashion2-woocommerce-tags">
            <fields>
                <field type="Woocommerce Tags: Title" editor_type="LINE">heading_title</field>
            </fields>
        </widget>

        <!-- furniture -->
        <widget name="tbay-furniture-custom-image-list-categories">
            <fields>
                <field type="Custom Image List Categories: Title" editor_type="LINE">heading_title</field>
                <field type="Custom Image List Categories: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="View All: Text" editor_type="LINE">view_all_text</field>
            </fields>
            <fields-in-item items_of="categoriestabs">
                <field type="Item: Label" editor_type="LINE">label</field>
            </fields-in-item>
        </widget>

        <!-- supermaket -->
        <widget name="tbay-supermaket-categories-tabs">
            <fields>
                <field type="Categories Tabs: Title" editor_type="LINE">heading_title</field>
                <field type="Categories Tabs: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="View All: Text" editor_type="LINE">view_all_text</field>
            </fields>
        </widget>

        <widget name="tbay-supermaket-features">
            <fields>
                <field type="features: Title" editor_type="LINE">heading_title</field>
                <field type="features: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="View More: Text" editor_type="LINE">view_more_text</field>
            </fields>
            <fields-in-item items_of="features">
                <field type="Item: Title" editor_type="LINE">feature_title</field>
                <field type="Item: Description" editor_type="AREA">feature_desc</field>
            </fields-in-item>
        </widget>

        <widget name="tbay-supermaket-products">
            <fields>
                <field type="products: Title" editor_type="LINE">heading_title</field>
                <field type="products: Sub Title" editor_type="LINE">heading_subtitle</field>
                <field type="Show More: Text" editor_type="LINE">text_button</field>
            </fields>
        </widget>

         <!-- supermaket2 -->
        <widget name="tbay-supermaket2-categoriestabs-2">
            <fields-in-item items_of="categoriestabs">
                <field type="Item Categoriestabs: Custom Name" editor_type="LINE">title</field>
            </fields-in-item>
        </widget>

         <widget name="tbay-supermaket2-categoriestabs-3">
            <fields>
                <field type="Categoriestabs: Title banner" editor_type="LINE">banner_title</field>
                <field type="Categoriestabs: Descriptions banner" editor_type="LINE">banner_des</field>
            </fields>
            <fields-in-item items_of="categoriestabs">
                <field type="Item Categoriestabs: Custom Name" editor_type="LINE">title</field>
            </fields-in-item>
        </widget>

        <widget name="tbay-supermaket2-categoriestabs">
            <fields>
                <field type="Categoriestabs: Title" editor_type="LINE">heading_title</field>
                <field type="Categoriestabs: Sub Title" editor_type="LINE">heading_subtitle</field>
            </fields>
            <fields-in-item items_of="categoriestabs">
                <field type="Item Categoriestabs: Custom Name" editor_type="LINE">title</field>
            </fields-in-item>
        </widget>

        <widget name="tbay-supermaket2-counter">
              <fields>
                <field type="Counter: Title" editor_type="LINE">title</field>
                <field type="Counter: Tag Name" editor_type="LINE">tag</field>
            </fields>
        </widget>


    </elementor-widgets>
</wpml-config>